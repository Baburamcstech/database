<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">

    <title>Add</title>
</head>
<body>
    <section class="h-100 h-custom" style="background-color: #8fc4b7;">
        <div class="container py-5 h-100">
          <div class="row d-flex justify-content-center align-items-center h-100">
            <div class="col-lg-8 col-xl-6">
              <div class="card rounded-3">
                <img src="https://mdbcdn.b-cdn.net/img/Photos/new-templates/bootstrap-registration/img3.webp"
                  class="w-100" style="border-top-left-radius: .3rem; border-top-right-radius: .3rem;"
                  alt="Sample photo">
                <div class="card-body p-4 p-md-5">
                  <h3 class="mb-4 pb-2 pb-md-0 mb-md-5 px-md-2">Registration Form</h3>
       
                  <form class="px-md-2" action="/register" method="post">
                 <%result.forEach(ele=>{%>

                    <div class="form-outline mb-4">
                        <label class="form-label" for="form3Example1q">Name</label>
                      <input type="text" id="form3Example1q" name="name" class="form-control" placeholder="<%=ele.username%>"/>
                    </div>
      
                    <div class="row mb-4 pb-2 pb-md-0 mb-md-5">
                      <div class="col-md-6">
      
                        <div class="form-outline">
                            <label class="form-label" for="form3Example1w">Email Id:</label>
                          <input type="text" id="form3Example1w" name="email" class="form-control" placeholder="<%=ele.email%>"/>
                        </div>
      
                      </div>
                    </div>
                    <div class="row mb-4 pb-2 pb-md-0 mb-md-5">
                      <div class="col-md-6">
      
                        <div class="form-outline">
                            <label class="form-label" for="form3Example1w">contact</label>
                          <input type="text" id="form3Example1w" name="contact" class="form-control" placeholder="<%=ele.contact%>"/>
                        </div>
      
                      </div>
                    </div>
                    <div class="row mb-4 pb-2 pb-md-0 mb-md-5">
                      <div class="col-md-6">
      
                        <div class="form-outline">
                            <label class="form-label" for="form3Example1w">Password</label>
                          <input type="text" id="form3Example1w" name="pass" class="form-control" />
                        </div>
      
                      </div>
                    </div>
      
                    <button type="submit" class="btn btn-success btn-lg mb-1">Submit</button><%})%>
      
                  </form>
      
                </div>
              </div>
            </div>
          </div>
        </div>
      </section>
    
</body>
</html>